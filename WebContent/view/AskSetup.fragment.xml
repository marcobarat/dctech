<core:FragmentDefinition
    id="AskSetup"
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
    xmlns:t="sap.ui.table">
    <Dialog contentWidth="1020px" contentHeight="320px" id="AskSetup" showHeader="false">
        <FlexBox alignItems="Start" justifyContent="Center">
            <VBox height="300px" width="1000px">
                <VBox height="48px"/>
                <VBox height="60px">
                    <FlexBox alignItems="Start" justifyContent="Center">
                        <Text text="Invio setup {ModelMachineSetup>/machine}" class="textTop"/>
                    </FlexBox>
                </VBox>
                <VBox height="60px"/>
                <VBox height="90px">
                    <HBox>
                        <VBox width="70px"/>
                        <VBox width="400px">
                            <Button text="Annulla" width="400px" press="AnnullaInvioSetup" class="annullaButtonDialog textButtonsDialog"/>
                        </VBox>
                        <VBox  width="60px">
                            <Text/>
                        </VBox>
                        <VBox width="400px">
                            <Button text="Conferma" width="400px" press="ConfermaInvioSetup" class="confermaButtonDialog textButtonsDialog"/>
                        </VBox>
                        <VBox width="70px"/>
                    </HBox>
                </VBox>
                <VBox height="42px"/>
            </VBox>
        </FlexBox>
    </Dialog>
</core:FragmentDefinition>